<?xml version="1.0" encoding="iso-8859-1"?>

	<slideshow>
	<settings>
		<panel width="600" height="400" />
		<wmode>window</wmode>
		<target>_blank</target>
		<main_image width="400" height="300" />
		<background>000000</background>
		<autoslide on="true" time="5" />
		<transition_speed>50</transition_speed>
		<transition_type>easeOut</transition_type>
		<description>
			<show>true</show>
			
			<location >bottom</location>
			<background>FF0000</background>
			<alpha>90</alpha>
			<text>FFFFFF</text>
			<roundness>0</roundness>
			
		</description>
		<border size="4" color="FFFFFF" />
		<reflection>image</reflection>
		<progress>
			<show>true</show>
			<location>top</location>
			<color>555555</color>
			<highlight>CCCCCC</highlight>
			<alpha>50</alpha>
		</progress>
	</settings>
